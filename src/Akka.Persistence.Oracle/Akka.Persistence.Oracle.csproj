<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Akka.NET Persistence journal and snapshot store backed by Oracle ODP.NET</Description>
    <AssemblyVersion>1.3.5.0</AssemblyVersion>
    <FileVersion>1.3.5.0</FileVersion>
    <RepositoryUrl>https://github.com/ismaelhamed/akka.persistence.oracle.git</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/ismaelhamed/akka.persistence.oracle/blob/master/LICENSE.md</PackageLicenseUrl>
    <Version>1.3.5</Version>
    <Authors>Ismael Hamed</Authors>
    <Company />
    <Copyright>Copyright © 2015-2018 Ismael Hamed</Copyright>
    <PackageProjectUrl>https://github.com/ismaelhamed/akka.persistence.oracle</PackageProjectUrl>
    <PackageTags>akka";"actors";"actor";"model";"Akka";"concurrency</PackageTags>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="oracle.conf" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="oracle.conf" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Akka" Version="1.4.0-beta1" />
    <PackageReference Include="Akka.Persistence" Version="1.4.0-beta1" />
    <PackageReference Include="Akka.Persistence.Sql.Common" Version="1.4.0-beta1" />
    <PackageReference Include="Oracle.ManagedDataAccess.Core" Version="2.19.31" />
  </ItemGroup>
</Project>
